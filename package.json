{
  "name": "enfunc-cli",
  "description": "High-availability distributed Function as a Service solution for building scalable apps and rest apis",
  "version": "0.0.0",
  "author": "Matisiek PL @MatisiekPL",
  "bin": {
    "enfunc": "./bin/run"
  },
  "bugs": "https://github.com/enteam/enfunc-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "globby": "^8",
    "rimraf": "^2.6.2",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/enteam/enfunc-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "enfunc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "enteam/enfunc-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
